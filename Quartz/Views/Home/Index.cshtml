@model QuartzLink

@{
    ViewData["Title"] = "Home Page";
}

<link href="~/css/site.css" rel="stylesheet" />
<link href="~/css/ol.css" rel="stylesheet" />

<!-- #region Home Page -->
<div class="container-fluid">
    <!-- #region Breadcrumbs -->
    <div class="row text-light text-center" style="background: #373a3e; height: 25px;">
        <div class="col-lg-2" style="border-right: 1px solid #212529;">
            <p> Lorem ipsum dolor, sit amet consectetur</p>
        </div>

        <div class="col-lg-8" style="border-right: 1px solid #212529; height: 25px;">
            <span>
                Lorem ipsum dolor, sit amet consectetur adipisicing elit. Sapiente ratione repellat officia ullam possimus
                pariatur atque sed iusto minus illo, facere placeat nisi. Officiis veniam
            </span>

        </div>

        <div class="col-lg-2">
            <p> Lorem ipsum dolor, sit amet consectetur</p>

        </div>
    </div>
    <!-- #endregion -->
    <!-- #region List Panel | Quartz Panel | Search Panel -->
    <div class="row">
        <div class="col-lg-2 px-0" id="listPanel" style="border: none;">
            @await Html.PartialAsync("/Views/Shared/Partial/HomePage/ListPanelPartial.cshtml")
        </div>

        <div class="col-lg-8 px-0" id="main">
            <div style="height: 85vh"></div>
            @*@await Html.PartialAsync("/Views/Shared/Partial/HomePage/QuartzPartial.cshtml")*@
        </div>

        <div class="col-lg-2 px-0">
            @await Html.PartialAsync("/Views/Shared/Partial/HomePage/SearchPanelPartial.cshtml")
        </div>
    </div>
    <!-- #endregion -->
</div>
<!-- #endregion -->
<!-- #region Link Modal -->
<div class="modal fade" id="linkModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
     aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content" style="height: 40vh;">
            <div class="modal-header" style="background-color: #F3F5F7;">
                <h5 class="modal-title" id="linkModalName">Link Modal</h5>
                <button type="button" class="btn-close closeButton" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="card-body" style="display: block;">

                        <div class="row">
                            <div class="col-lg-6">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Tag No</span>
                                <input type="text" id="addLinkTagNo" class="form-control">
                            </div>

                            <div class="col-lg-6">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Current Drawing</span>
                                <input type="text" id="addLinkCurrentDrawing" class="form-control" value="Drawing doesn't exist!" disabled style="background-color: white; border:1px dashed gray;">
                            </div>
                        </div>

                        <br>
                        <hr>

                        <div class="row">
                            <div class="text-center">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 "></span>
                                <button id="btnAddLinkUploadDrawing" type="button" class="btn btn-dark" style="border: none; width:150px; border-radius: 20px;">Upload Drawing</button>
                            </div>
                        </div>

                        <div class="row">
                            <div class="text-center">
                                <br>
                                <div id="AddLinkUploadDrawingArea" class="row" hidden>
                                    <div class="col-lg-12">
                                        <label for="addLinkUploadDrawing" style="border-radius: 20px;" class="btn btn-primary">Choose</label>
                                        <input id="addLinkUploadDrawing" type="file" hidden>
                                        <button type="button" class="btn btn-success" style="border-radius: 20px; color: white;" onclick="uploadFile(lastClickedLinkButtonId, 'link')">Upload</button>
                                    </div>
                                </div>
                                <br />
                            </div>
                        </div>
                    </div>
                </form>
            </div>

            <div class="modal-footer" style="background-color: #F3F5F7;">
                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Show Label</span>
                <input id="linkShowLabel" type="checkbox" name="showLabel" /> &nbsp;&nbsp;
                <button type="button" class="btn btn-dark closeButton" data-bs-dismiss="modal" style="border: none; border-radius: 0px;">Close</button>
                <button type="button" class="btn btn-danger" style="border:none; border-radius: 0px;">Delete</button>
                <button type="button" class="btn btn-primary" style="border: none; border-radius: 0px;" onclick="linkModalSaveButton()" data-bs-dismiss="modal">Save</button>
            </div>
        </div>
    </div>
</div>
<!-- #endregion -->
<!-- #region Item Modal -->
<div class="modal fade" id="itemModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #F3F5F7;">
                <div class="col-lg-3">
                    <h5 class="modal-title" id="itemModalTitle">Item Modal</h5>
                </div>
                <div class="col-lg-6">
                    <ul class="spec nav bg-white" style="border-radius: 20px;" role="tablist" id="itemModalNav">
                        <li class="active nav-item"><a class="nav-link text-dark" href="#itemModalInformations" data-toggle="tab">Informations</a></li>
                        <li class="nav-item disabled"><a class="nav-link" href="#" data-toggle="tab" style="color: gray;">|</a></li>
                        <li class="nav-item"><a role="tab" class="nav-link text-dark" href="#itemModalInspections" data-toggle="tab">Inspections</a></li>
                        <li class="nav-item disabled"><a class="nav-link" href="#" data-toggle="tab" style="color: gray;">|</a></li>
                        <li class="nav-item"><a role="tab" class="nav-link text-dark" href="#itemModalAttachments" data-toggle="tab">Attachments</a></li>
                    </ul>
                </div>
                <div class="col-lg-3">
                    <button type="button" class="btn-close float-end mx-1 closeButton" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>

            </div>
            <div class="modal-body">
                <div class="row">

                </div>

                <div class="row" id="itemModalPartialArea">

                </div>
            </div>
            <div class="modal-footer" style="background-color: #F3F5F7;">
                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Show Label</span>
                <input id="itemShowLabel" type="checkbox" name="showLabel" /> &nbsp;&nbsp;
                <button type="button" class="btn btn-danger" style="border:none; border-radius:0px;">Delete</button>
                <button type="button" class="btn btn-primary" style="border:none; border-radius:0px;" onclick="itemModalSaveButton(itemModalActivePartial)" data-bs-dismiss="modal" aria-label="Close">Save</button>
            </div>
        </div>
    </div>
</div>
<!-- #region Inspection Add (Button) Modal -->
<div class="modal fade" id="AddInspectionData" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content">

            <div class="modal-header" style="background-color: #F3F5F7;">
                <h2 class="modal-title">Inspection Data</h2>
                <button type="button" class="btn-close float-end mx-1" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>

            <div class="modal-body">

                <div class="row">
                    <div class="col-lg-12">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Report No</span>
                        <input type="text" id="inspectionReportNo" class="form-control" placeholder="Enter Report No" style="background-color: #F3F5F7;">
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Date</span>
                        <input type="date" id="inspectionDate" class="form-control" style="background-color: #F3F5F7;">
                    </div>

                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Due Date</span>
                        <input type="date" id="inspectionDateDue" class="form-control" style="background-color: #F3F5F7;">
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Method</span>
                        <select class="form-select" id="inspectionMethod" style="background-color: #F3F5F7;">
                        </select>
                    </div>

                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Procedure</span>
                        <select class="form-select" id="inspectionProcedure" style="background-color: #F3F5F7;">
                        </select>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Technique</span>
                        <select class="form-select" id="inspectionTechnique" style="background-color: #F3F5F7;">
                        </select>
                    </div>

                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Status</span>
                        <select class="form-select" id="inspectionStatus" style="background-color: #F3F5F7;">
                        </select>
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-12">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Details</span>
                        <textarea id="inspectionDetails" class="form-control" style="min-height: 40px; width: 100%; max-height: 250px;background-color: #F3F5F7;" maxlength="1000" rows="2" placeholder="Enter Detail"></textarea>
                    </div>
                </div>
                <br />
                <div class="form-row">
                    <div class="form-group col-sm-2">
                        <label for="inspectionFileUpload" style="color: gray; font-weight: 400; border: 1px solid black; padding: 5px;" class="custom-file-upload">
                            <i style="color:#364F6B;" class="fa fa-cloud-upload"></i> Choose File
                        </label>
                        <input id="inspectionFileUpload" type="file" name="files" hidden />
                    </div>
                    <div class="col-sm-10">
                        <input class="form-control" style="width: 100%; border: 1px solid black; border-radius: 0px; padding: 5px;" type="text" autocomplete="off" placeholder="Description..." name="description" required />
                    </div>
                </div>

            </div>

            <div class="modal-footer" style="background-color: #F3F5F7;">
                <button type="button" class="btn btn-dark" data-bs-dismiss="modal" style="border:none; border-radius:0px;">Close</button>
                <button type="button" class="btn btn-primary" style="border:none; border-radius: 0px;" id="inspectionAddSaveButton" data-bs-dismiss="modal">Save</button>
            </div>
        </div>
    </div>
</div>
<!-- #endregion -->
<!-- #endregion -->
<!-- #region Drawing Settings Modal (#DSM) -->
<div class="modal fade" id="drawingSettingsModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" , aria-hidden="true">
