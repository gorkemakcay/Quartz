<!-- #region Link Modal -->
<div class="modal fade" id="linkModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
     aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content minHeight">
            <div class="modal-header" style="background-color: #F3F5F7;">
                <h5 class="modal-title" id="linkModalName">Link Modal</h5>
                <button type="button" class="btn-close closeButton" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="card-body" style="display: block;">

                        <div class="row">
                            <div class="col-lg-6">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Tag No</span>
                                <input type="text" id="addLinkTagNo" class="form-control">
                            </div>

                            <div class="col-lg-6">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Select Drawing</span>
                                <select id="addLinkSelectDrawing" class="form-select" onblur='this.size=1;' onchange='this.size=1; this.blur();'>
                                </select>
                            </div>
                        </div>

                        <br />
                        <hr />
                        <br />

                        @* --- CLICKED LINK AREA --- *@

                        <div class="row" id="clickedLinkMode">
                            <div class="text-center col-lg-6" style="border-right: 1px solid #8F8F95;">
                                <button id="addLinkGoCurrentLinkButton" type="button" class="btn btn-dark" style="border: none; width:150px; border-radius: 20px;" onclick="goCurrentLink()" data-bs-dismiss="modal">Go Current Link</button>
                            </div>

                            <div class="text-center col-lg-6">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 "></span>
                                <button id="btnAddLinkUploadDrawing" type="button" class="btn btn-dark" style="border: none; width:150px; border-radius: 20px;">Upload Drawing</button>
                            </div>
                        </div>

                        <div class="row" style="margin-top: -2px;" id="AddLinkUploadDrawingArea" hidden>
                            <div class="text-center col-lg-6">
                            </div>

                            <div class="text-center col-lg-6">
                                <br />
                                <div class="row">
                                    <br>
                                    <div class="col-lg-12">
                                        <label for="addLinkUploadDrawing" style="border-radius: 20px;" class="btn btn-dark"><i class='bi bi-file-image-fill'></i>&nbsp; Choose</label>
                                        <input id="addLinkUploadDrawing" type="file" hidden>
                                        <button type="button" class="btn btn-dark" style="border-radius: 20px; color: white;" onclick="uploadFile('link')"><i class="bi bi-upload"></i>&nbsp; Upload</button>
                                    </div>
                                </div>

                                <div class="row text-center">
                                    <p class="linkModalSelectedDrawing"></p>
                                </div>
                            </div>
                        </div>
                        @* --- CLICKED LINK AREA --- *@

                        @* --- CREATED LINK AREA --- *@
                        <div class="row" id="createdLinkMode" hidden>
                            <div class="text-center col-lg-12">
                                <span style="color: #8F8F95; font-size: 1em; font-weight:500 "></span>
                                <button id="btnAddLinkUploadDrawingCreatedMode" type="button" class="btn btn-dark" style="border: none; width:150px; border-radius: 20px;">Upload Drawing</button>
                            </div>
                        </div>

                        <div class="row" style="margin-top: -2px;" id="AddLinkUploadDrawingAreaCreatedMode" hidden>
                            <div class="text-center col-lg-12">
                                <br />
                                <div class="row">
                                    <br>
                                    <div class="col-lg-12">
                                        <label for="addLinkUploadDrawing" style="border-radius: 20px;" class="btn btn-dark"><i class='bi bi-file-image-fill'></i>&nbsp; Choose</label>
                                        <input id="addLinkUploadDrawing" type="file" hidden>
                                        <button type="button" class="btn btn-dark" style="border-radius: 20px; color: white;" onclick="uploadFile('link')"><i class="bi bi-upload"></i>&nbsp; Upload</button>
                                    </div>
                                </div>

                                <div class="row text-center">
                                    <p class="linkModalSelectedDrawing"></p>
                                </div>
                            </div>
                        </div>
                        @* --- CREATED LINK AREA --- *@
                    </div>
                </form>
            </div>

            <div class="modal-footer" style="background-color: #F3F5F7;">
                <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Show Label</span>
                <input id="linkShowLabel" type="checkbox" name="showLabel" /> &nbsp;&nbsp;
                <button type="button" class="btn btn-dark closeButton" data-bs-dismiss="modal" style="border: none; border-radius: 0px;">Close</button>
                <button type="button" class="btn btn-dark" style="border: none; border-radius: 0px;" onclick="linkModalSaveButton()" data-bs-dismiss="modal">Save</button>
            </div>
        </div>
    </div>
</div>
