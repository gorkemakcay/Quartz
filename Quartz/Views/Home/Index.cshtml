                    </div>

                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Drawing Description</span>
                        <input type="text" id="dsmDrawingDescription" class="form-control">
                    </div>
                </div>

                <br /><br />

                <div class="row">
                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Plant Area</span>
                        <select class="form-select" id="dsmPlantArea">
                        </select>
                    </div>

                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 ">Plant System</span>
                        <select class="form-select" id="dsmPlantSystem">
                        </select>
                    </div>
                </div>

                <br />

                <div class="row">
                    <div class="col-lg-6">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500">Drawing File: </span>
                        <input type="text" id="dsmFile" class="" style="border: none; background-color: white;" disabled value="file not found!">
                    </div>

                    <div class="col-lg-6">

                    </div>
                </div>

                <br />

                <!-- #region Upload Drawing Button -->
                <div class="row">
                    <div class="text-center">
                        <span style="color: #8F8F95; font-size: 1em; font-weight:500 "></span>
                        <button id="btnDsmUploadDrawing" type="button" class="btn btn-dark" style="border: none; width:150px; border-radius: 20px;">Upload Drawing</button>
                    </div>
                </div>
                <!-- #endregion -->
                <!-- #region Upload Drawing Area -->
                <div class="row">
                    <div class="text-center">
                        <br>
                        <div id="dsmUploadDrawingArea" class="row" hidden>
                            <div class="col-lg-12">
                                <label for="dsmUploadDrawing" style="border-radius: 20px;" class="btn btn-primary">Choose</label>
                                <input id="dsmUploadDrawing" type="file" hidden>
                                <button type="button" class="btn btn-success" style="border-radius: 20px; color: white;" onclick="uploadFile(currentDrawingSettings.Id, 'drawingSettings')">Upload</button>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
                <!-- #endregion -->
            </div>

            <!-- #region Footer -->
            <div class="modal-footer" style="background-color: #F3F5F7;">
                <button type="button" class="btn btn-danger">Delete</button>
                <button type="button" class="btn btn-primary" id="btnDsmSave">Save</button>
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
            </div>
            <!-- #endregion -->

        </div>
    </div>
</div>
