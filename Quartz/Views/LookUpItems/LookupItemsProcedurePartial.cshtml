<table id="procedureTable" class="display" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Method</th>
        </tr>
    </thead>

    <tbody>
        @if (ViewBag.Procedures != null)
        {
            foreach (var procedure in ViewBag.Procedures)
            {
                <tr>
                    <td>@procedure.Name</td>
                    <td>@procedure.LookUpItemsMethod</td>
                </tr>
            }
        }
    </tbody>
</table>

<br />

<div class="modal-footer">
    <br />
    <button type="button" class="btn btn-success" data-bs-toggle="modal" data-bs-target="#limProcedureAddModal">+ Add</button>
    <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
</div>
